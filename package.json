{
  "name": "paracord",
  "version": "0.0.5",
  "description": "Scalable Discord gateway and API handlers alongside a pre-built client.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc -b && cp -r src/rpc/protobufs dist/rpc/protobufs",
    "build:types": "tsc -p ./tsconfig.json --declaration true --outDir build && api-extractor run",
    "rebuild": "npm run build && npm run build:types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paracordjs/paracord.git"
  },
  "author": "Landeau McDade",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/paracordjs/paracord/issues"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@grpc/proto-loader": "^0.6.13",
    "@types/ws": "^8.5.3",
    "axios": "^0.21.4",
    "form-data": "^4.0.0",
    "protobufjs": "^6.11.2",
    "ws": "^8.8.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.5",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "pm2": "^5.2.0"
  },
  "peerDependenciesMeta": {
    "pm2": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.6",
    "utf-8-validate": "^5.0.9",
    "zlib-sync": "^0.1.7"
  },
  "keywords": [
    "discord",
    "discord",
    "api",
    "bot",
    "client",
    "node",
    "paracord"
  ]
}
